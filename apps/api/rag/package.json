{
    "name": "@buildbox/rag",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "bun --watch src/index.ts",
        "run": "bun --watch src/ai/agents/summary-agent.ts",
        "flash": "bun --watch src/ai/agents/flashcard-agent.ts",
        "docs": "bun --watch src/services/documents-acess-service.ts",
        "build": "bun run --bun tsc",
        "start": "bun dist/src/index.js",
        "ingest": "bun --watch utilities/ingest.ts",
        "ingest-optimized": "bun --watch utilities/ingestOptimized.ts",
        "digest": "bun --watch utilities/digest.ts",
        "test-rag": "bun test-rag.js",
        "type-check": "bun run --bun tsc --noEmit",
        "clean": "rm -rf dist",
        "lint": "echo 'RAG service - no lint step required'"
    },
    "dependencies": {
        "@google/generative-ai": "^0.24.1",
        "@langchain/community": "^0.3.46",
        "@langchain/core": "^0.3.58",
        "@langchain/google-genai": "^0.2.12",
        "@langchain/openai": "^0.0.14",
        "@langchain/qdrant": "^0.0.3",
        "@qdrant/js-client-rest": "^1.0.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "langchain": "^0.3.28",
        "minio": "^8.0.5",
        "pdf-parse": "^1.1.1",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.0"
    },
    "devDependencies": {
        "@google/generative-ai": "^0.3.1",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.0",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.6",
        "typescript": "^5.3.2"
    }
}